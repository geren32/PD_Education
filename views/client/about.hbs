
    <main>

        <!-- POST -->
        <div class="animate-item">
            <div class="single-post-img">
                <div class="overflow-hidden">
<!--                    <div class="rellax" style="background-image: url('/img/about.jpg');"></div>-->
                    <div class="rellax" style="background-image: url({{page.image}});"></div>
                </div>
                <div class="container">
<!--                    <h1 class="h2 single-post-title text-center color-white">Про нас</h1>-->
                    <h1 class="h2 single-post-title text-center color-white">{{page.title}}</h1>
                </div>
            </div>
            <div class="container">
                <div class="tabs">
                    <div class="tab-nav">
                        <div class="row">
                            <div class="col-xl-8 offset-xl-2">
                                <div class="tab-toggle d-flex justify-content-lg-center">
                                    {{#each page.sections}}
                                        -------------------{{../page.sections.length}}-------------------
                                        <div {{#ifCond @index "==" "0"}} class="active" {{/ifCond}}>{{this.title}}</div>
                                    {{/each}}
<!--                                    <div>Наші Переваги</div>-->
<!--                                    <div>досягнення</div>-->
<!--                                    <div>Навчання</div>-->
<!--                                    <div>Сервіси</div>-->
<!--                                    <div>Презентація</div>-->
<!--                                    <div>Часті запитання</div>-->
                                </div>
                            </div>
                        </div>
                    </div>


                    {{#each page.sections}}
                    <div class="tab">
                        {{#each this.body}}
                        {{#switch this.type}}
                            {{#case '1'}}
                            <div class="row">
                                <div class="col-xl-8 offset-xl-2">
                                    <div class="simple-page p-0">
                                        {{{this.content}}}
                                    </div>
                                </div>
                            </div>
                            {{/case}}
                            {{#case '2'}}
                                <div class="row">
                                    <div class="col-xl-8 offset-xl-2">
                                        <div class="simple-page pt-0">
                                            <div class="simple-swiper">
                                                <div class="swiper-entry">
                                                    <div class="swiper-container" data-options='{"slidesPerView":1,"autoHeight":true}'>
                                                        <div class="swiper-wrapper">
                                                            {{#each this.content}}
                                                            <div class="swiper-slide">
                                                                <div class="img-wrapp"><img src="{{this.image}}" alt="" class="img"></div>
                                                                <figure class="simple-figure m-0">
                                                                    <figcaption>{{this.text}}</figcaption>
                                                                </figure>
                                                            </div>
                                                            {{/each}}
                                                        </div>
                                                        <div class="swiper-pagination"></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            {{/case}}
                            {{#case '3'}}
                                <div class="section pt-0">
                                {{#each this.content}}
                                {{#ifCond this.left "==" "true"}}
                                    <div class="row benefit">
                                        <div class="col-xl-7 offset-xl-1">
                                            <div class="pr-xl-27"><img src="/img/preloader.jpg" data-i-src="{{this.image}}" alt="" class="img"></div>
                                        </div>
                                        <div class="col-xl-3">
                                            <div class="benefit-desc d-flex flex-column justify-content-center">
                                                <div class="benefit-title type-1 d-flex align-items-center h4">{{this.title}}</div>
                                                <p>{{this.text}}</p>
                                            </div>
                                        </div>
                                    </div>
                                {{else}}
                                        <div class="row benefit">
                                            <div class="col-xl-3 offset-xl-1 order-2 order-xl-1">
                                                <div class="benefit-desc d-flex flex-column justify-content-center">
                                                    <div class="benefit-title type-1 d-flex align-items-center h4">{{this.title}}</div>
                                                    <p>{{this.text}}</p>
                                                </div>
                                            </div>
                                            <div class="col-xl-7 order-1 order-xl-2">
                                                <div class="pr-xl-27"><img src="/img/preloader.jpg" data-i-src="{{this.image}}" alt="" class="img"></div>
                                            </div>
                                        </div>
                                {{/ifCond}}
                                {{/each}}
                                </div>
                            {{/case}}
                            {{#case '4'}}
                                <div class="mobile-slider mobile-fullwidth">
                                    <div class="swiper-entry">
                                        <div class="swiper-container" data-options='{"slidesPerView":2, "spaceBetween": 15, "pagination": {"type": "fraction", "el": ".swiper-pagination"}, "breakpoints": {"1199": {"slidesPerView": 3, "pagination": {"type": "fraction"}}}}'>
                                            <div class="swiper-button-prev d-none"></div>
                                            <div class="swiper-button-next d-none"></div>
                                            <div class="swiper-wrapper row justify-content-xl-center flex-nowrap flex-xl-wrap">
                                                {{#each this.content}}
                                                <div class="swiper-slide col-xl-3">
                                                    <div class="circle">
                                                        <div class="circle-entry d-flex flex-column justify-content-center">
                                                            <div class="circle-icon"><img src="/img/preloader.jpg" data-i-src="{{this.image}}" alt=""></div>
                                                            <div class="circle-title">{{this.title}}</div>
                                                            <div class="circle-excerpt text-truncate line-2">{{this.text}}</div>
                                                        </div>
                                                    </div>
                                                </div>
                                                {{/each}}
                                            </div>
                                            <div class="swiper-pagination d-xl-none"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="swiper-pagination d-xl-none"></div>
                                <div class="spacer-sm"></div>
                            {{/case}}
                            {{#case '5'}}
                                <div class="section pt-0">
                                    <div class="row cols-3">
                                        {{#each this.content}}
                                        <div class="col-md-6 col-lg-4">
                                            <div class="post">
                                                <div class="post-img size-1"><a href="post/{{this.link}}"><img src="{{this.image}}" alt="" class="img"></a></div>
                                                <div class="post-info">
                                                    <a href="post/{{this.link}}" class="h5 post-title d-block">{{this.title}}</a>
                                                    <div class="post-excerpt text-truncate line-3">{{this.text}}</div>
                                                </div>
                                            </div>
                                        </div>
                                        {{/each}}
                                    </div>
                                    <div class="spacer-sm"></div>
                                </div>
                            {{/case}}
                            {{#case '6'}}
                                <div class="section pt-0">
                                    <div class="row">
                                        <div class="col-xl-8 offset-xl-2">
                                            <div class="accordion">
                                                {{#each this.content}}
                                                <div class="accordion-item">
                                                    <div class="h5 accordion-title">{{this.title}}</div>
                                                    <div class="accordion-inner">
                                                        <div class="simple-page p-0">
                                                          {{{this.text}}}
                                                        </div>
                                                    </div>
                                                </div>
                                                {{/each}}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            {{/case}}
                        {{/switch}}
                        {{/each}}
                        {{#ifCond @index "==" "6"}}
                        <div class="section pt-0">
                            <div class="row">
                            <div class="col-xl-8 offset-xl-2">
                                <div class="box">
                                    <div class="h4 box-title">Залишились питання? Напишіть нам</div>
                                    <form>
                                        <div class="row type-1 mb-20">
                                            <div class="col-md-4 mb-20 mb-md-0"><input type="text" placeholder="Ваше ім’я" class="input" required></div>
                                            <div class="col-md-4 mb-20 mb-md-0"><input type="tel" placeholder="+38 (098) 000 00 00" class="input inputmask" inputmode="numeric" data-inputmask="'mask': '+38 (0x9) 999 99 99'" data-inputmask-placeholder="x" required autocomplete="new-password"></div>
                                            <div class="col-md-4"><input type="email" placeholder="Email" class="input"></div>
                                        </div>
                                        <textarea placeholder="Ваше запитання ..." class="input"></textarea>
                                        <div class="text-right mt-23">
                                            <button class="btn btn-primary type-1">Надіслати</button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                            </div>
                        </div>
                        {{/ifCond}}
                    </div>
                    {{/each}}
                    </div>
                </div>
            </div>
    </main>

