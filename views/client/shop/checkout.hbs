

    <main>

    {{#ifCond cart '==' 'кошик пустий'}}
        <section class="section section-404">
            <div class="container">
                <div class="row">
                    <div class="col-12 text-center">
                        <div class="h2 section-title">Ваш кошик порожній</div>
                        <a href="/" class="btn btn-primary btn-invert">На головну</a>
                    </div>
                </div>
            </div>
        </section>
    {{else}}
        <div class="section checkout-section">
            <div class="container">
                <div class="row">
                    <div class="col-xl-8 offset-xl-2">
                        <h1 class="h2 section-title">Оформлення замовлення</h1>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-7 col-xl-6 offset-xl-1">
                        <div class="checkout-products" id="CheckoutAjax">

                            {{#each result.products}}
                                <div class="item-wrapp" data-price="{{this.variation.price}}">
                                    <div class="item d-flex">
                                        <div class="item-img"><a href="{{this.product.slag}}"><img src="{{this.product.image}}" alt="" class="img"></a></div>
                                        <div class="item-caption">
                                            <div class="card-code">{{product.sku}}</div>
                                            <div class="h5 item-title"><a href="{{this.product.slag}}">{{this.product.name}}</a></div>
                                            <p>{{this.product.short_description}}</p>
                                            <div class="d-flex justify-content-between">
                                                <div class="thumb-input-number">
                                                    <button type="button" class="add-product-to-checkout" product="{{this.order.product_id}}" variation="{{this.order.variation_id}}" data="-1">-</button>
                                                    <input value="{{this.order.count}}" readonly="" tabindex="-1">
                                                    <button type="button" class="add-product-to-checkout" product="{{this.order.product_id}}" variation="{{this.order.variation_id}}" data="1">+</button>
                                                </div>
                                                <div class="h5 item-price"><b class="price">{{this.variation.price}}</b> грн</div>
                                            </div>
                                        </div>
                                        <button class="btn-close delete-order" order_id = '{{this.order.id}}'><svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <path d="M12 4L4 12" stroke-width="1.5" stroke-linecap="square" />
                                            <path d="M4 4L12 12" stroke-width="1.5" stroke-linecap="square" />
                                        </svg>
                                        </button>
                                    </div>
                                </div>
                            {{/each}}

                            {{#each result.kits}}
                                <div class="item-wrapp" data-price="{{this.price}}">
                                    <div class="item d-flex">
                                        <div class="item-img"><a href="{{this.kit.slag}}"><img src="{{this.kit.image}}" alt="" class="img"></a></div>
                                        <div class="item-caption">
                                            <div class="card-code">{{this.kit.sku}}</div>
                                            <div class="h5 item-title"><a href="{{this.kit.slag}}">{{this.kit.name}}</a></div>
                                            <p>{{this.kit.short_description}}</p>
                                            <div class="d-flex justify-content-between">
                                                <div class="thumb-input-number">
                                                    <button type="button" class="add-product-kit-to-checkout" product_kit_id="{{this.kit.id}}" kit="{{this.productsJson}}" data="-1">-</button>
                                                    <input value="{{this.order.count}}" readonly="" tabindex="-1">
                                                    <button type="button" class="add-product-kit-to-checkout" product_kit_id="{{this.kit.id}}" kit="{{this.productsJson}}" data="1">+</button>
                                                </div>
                                                <div class="h5 item-price"><b class="price">{{this.price}}</b> грн</div>
                                            </div>
                                        </div>
                                        <button class="btn-close delete-order" order_kit_id = '{{this.order.id}}'><svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <path d="M12 4L4 12" stroke-width="1.5" stroke-linecap="square" />
                                            <path d="M4 4L12 12" stroke-width="1.5" stroke-linecap="square" />
                                        </svg>
                                        </button>
                                    </div>
                                    <table width="100%" cellspacing="0" cellpadding="0">
                                        {{#each this.products}}
                                            <tr>
                                                <td>{{this.product.name}}</td>
                                                <td class="nowrap">{{this.info.quantity}} шт.</td>
<!--                                                <td>Коричневий</td>-->
                                            </tr>
                                        {{/each}}
                                    </table>
                                    <div class="item-comment">
                                        {{#if this.order.comment includeZero=true}}
                                            <div data-block="3" data-rel="0">
                                                <label class="label">Коментар</label>
                                                <div class="comment">
                                                    <p>{{this.order.comment}}</p>
                                                    <button class="btn-link size-1 d-flex align-items-center mt-15 delete-comment add-product-kit-to-checkout" comment="" product_kit_id="{{this.kit.id}}" kit="{{this.productsJson}}" data="0"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                        <path fill-rule="evenodd" clip-rule="evenodd" d="M9.11612 3.11612C9.35054 2.8817 9.66848 2.75 10 2.75H14C14.3315 2.75 14.6495 2.8817 14.8839 3.11612C15.1183 3.35054 15.25 3.66848 15.25 4V5.25H8.75V4C8.75 3.66848 8.8817 3.35054 9.11612 3.11612ZM7.25 5.25V4C7.25 3.27065 7.53973 2.57118 8.05546 2.05546C8.57118 1.53973 9.27065 1.25 10 1.25H14C14.7293 1.25 15.4288 1.53973 15.9445 2.05546C16.4603 2.57118 16.75 3.27065 16.75 4V5.25H19H21C21.4142 5.25 21.75 5.58579 21.75 6C21.75 6.41421 21.4142 6.75 21 6.75H19.75V20C19.75 20.7293 19.4603 21.4288 18.9445 21.9445C18.4288 22.4603 17.7293 22.75 17 22.75H7C6.27065 22.75 5.57118 22.4603 5.05546 21.9445C4.53973 21.4288 4.25 20.7293 4.25 20V6.75H3C2.58579 6.75 2.25 6.41421 2.25 6C2.25 5.58579 2.58579 5.25 3 5.25H5H7.25ZM5.75 6.75V20C5.75 20.3315 5.8817 20.6495 6.11612 20.8839C6.35054 21.1183 6.66848 21.25 7 21.25H17C17.3315 21.25 17.6495 21.1183 17.8839 20.8839C18.1183 20.6495 18.25 20.3315 18.25 20V6.75H16H8H5.75ZM10 10.25C10.4142 10.25 10.75 10.5858 10.75 11V17C10.75 17.4142 10.4142 17.75 10 17.75C9.58579 17.75 9.25 17.4142 9.25 17V11C9.25 10.5858 9.58579 10.25 10 10.25ZM14.75 11C14.75 10.5858 14.4142 10.25 14 10.25C13.5858 10.25 13.25 10.5858 13.25 11V17C13.25 17.4142 13.5858 17.75 14 17.75C14.4142 17.75 14.75 17.4142 14.75 17V11Z" />
                                                    </svg>Видалити</button>
                                                </div>
                                            </div>
                                        {{else}}
                                            <div class="checkbox-entry-wrap">
                                                <label class="checkbox-entry style-1">
                                                    <input type="checkbox" name="addComment" class="toggle-block-control" data-block="3" data-rel="0"><span class="d-flex align-items-center"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                        <path d="M21 11.5C21.0034 12.8199 20.6951 14.1219 20.1 15.3C19.3944 16.7118 18.3098 17.8992 16.9674 18.7293C15.6251 19.5594 14.0782 19.9994 12.5 20C11.1801 20.0035 9.87812 19.6951 8.7 19.1L3 21L4.9 15.3C4.30493 14.1219 3.99656 12.8199 4 11.5C4.00061 9.92179 4.44061 8.37488 5.27072 7.03258C6.10083 5.69028 7.28825 4.6056 8.7 3.90003C9.87812 3.30496 11.1801 2.99659 12.5 3.00003H13C15.0843 3.11502 17.053 3.99479 18.5291 5.47089C20.0052 6.94699 20.885 8.91568 21 11V11.5Z" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" />
                                                        <path fill-rule="evenodd" clip-rule="evenodd" d="M13.2532 8.74964V8H11.7525V8.74964V10.7479H9.75232H9.00195V12.2471L9.75232 12.2471H11.7525V14.2454L11.7525 14.995H13.2532V14.2454V12.2471H15.2534L16.0038 12.2471L16.0038 10.7479H15.2534H13.2532V8.74964Z" />
                                                    </svg>Додати коментар</span>
                                                </label>
                                            </div>
                                            <div class="toggle-block" data-block="3" data-rel="0">
                                                <label class="label">Коментар</label>
                                                <div class="comment-wrapp">
                                                    <textarea name="comment-textarea" id="{{this.kit.id}}" class="input" placeholder="Введіть текст..." spellcheck="false"></textarea>
                                                    <div class="d-flex justify-content-end">
                                                        <button class="btn btn-link cancel-comment">Скасувати</button>
                                                        <button class="btn btn-primary type-1 add-comment add-product-kit-to-checkout" product_kit_id="{{this.kit.id}}" kit="{{this.productsJson}}" data="0">Зберегти</button>
                                                    </div>
                                                </div>
                                            </div>
                                        {{/if}}
                                    </div>
                                </div>
                            {{/each}}


